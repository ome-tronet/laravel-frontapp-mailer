{
    "name": "ome-tronet/laravel-frontapp-mailer",
    "description": "Laravel mail driver for Front customer service platform",
    "keywords": [
        "Oliver Merklinghaus",
        "laravel",
        "laravel-frontapp-mailer"
    ],
    "homepage": "https://github.com/ome-tronet/laravel-frontapp-mailer",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Merklinghaus",
            "email": "ome@tro.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "tronet\\FrontappMailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tronet\\FrontappMailer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "tronet\\FrontappMailer\\FrontAppServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
